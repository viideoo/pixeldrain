<script>
    // Script para mostrar u ocultar el texto en dispositivos m  viles
    if (window.innerWidth <= 768) {
        document.querySelector('.desktop-text').style.display = 'none';
        document.querySelector('.mobile-text').style.display = 'inline-flex';
    }

    // Funci  n para alternar la visibilidad del men   (puedes personalizarla seg  n tus necesidades)
    function toggleMenu() {
        var menuOptions = document.getElementById('menuOptions');
        menuOptions.style.display = (menuOptions.style.display === 'block') ? 'none' : 'block';
    }
</script>

